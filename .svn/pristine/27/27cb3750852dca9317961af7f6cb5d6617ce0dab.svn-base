.cell-swiper {
  position: relative;
}

.grid-overlay {
  position: absolute;
  /* 绝对定位叠加 */
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  grid-template-rows: repeat(3, 1fr);
  pointer-events: none;
}

.grid-cell {
  border: 0.5px solid rgba(255, 255, 255, 0.5);
  box-sizing: border-box;
}

/* 提高选择器优先级 */
.reply-image {
  background-size: contain !important;
}