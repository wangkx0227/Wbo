<template name="skeleton">
  <view class="sk-container">
    <view class="page-wrapper">
      <view style="background-color: #ffff;">
        <t-row>
          <t-col span="24">
            <view style="height: 49px;display: flex;border-bottom: 1px solid #f5f5f5;">
              <view style="width: 50%;height: 100%;display: flex;align-items: center;justify-content: center;">
                <t-skeleton theme="paragraph" loading animation="gradient" rowCol="{{ [{ width: '56px', height: '19px' }]}}"></t-skeleton>
              </view>
              <view style="width: 50%;height: 100%;display: flex;align-items: center;justify-content: center; ">
                <t-skeleton theme="paragraph" loading animation="gradient" rowCol="{{ [{ width: '56px', height: '19px' }]}}"></t-skeleton>
              </view>
            </view>
          </t-col>
        </t-row>
      </view>
      <view class="page-main" style="background-color: #ffff;height: 90vh;">
        <view class="wrapper" >
          <view wx:for="{{ [1,2,3,4,5,6,7,8] }}" wx:for-item="item" wx:key="index">
            <t-skeleton theme="paragraph" loading animation="gradient" rowCol="{{ [{ width: '760rpx', height: '154rpx',borderRadius: '10rpx' }]}}" style="margin-bottom: 15rpx;"></t-skeleton>
          </view>
        </view>
      </view>
    </view>
  </view>
</template>