<template name="skeleton">
  <view class="sk-container">
    <view class="page-wrapper">
      <view style="background-color: #ffff;">
        <t-row>
          <t-col span="24">
            <view style="height: 49px;display: flex;border-bottom: 1px solid #f5f5f5;">
              <view style="width: 50%;height: 100%;display: flex;align-items: center;justify-content: center;">
                <t-skeleton theme="paragraph" loading animation="gradient" rowCol="{{ [{ width: '56px', height: '19px' }]}}"></t-skeleton>
              </view>
              <view style="width: 50%;height: 100%;display: flex;align-items: center;justify-content: center; ">
                <t-skeleton theme="paragraph" loading animation="gradient" rowCol="{{ [{ width: '56px', height: '19px' }]}}"></t-skeleton>
              </view>
              <view style="width: 50%;height: 100%;display: flex;align-items: center;justify-content: center; ">
                <t-skeleton theme="paragraph" loading animation="gradient" rowCol="{{ [{ width: '56px', height: '19px' }]}}"></t-skeleton>
              </view>
            </view>
          </t-col>
        </t-row>
      </view>
      <view class="page-main">
        <view class="wrapper">
          <view wx:for="{{ [1] }}" wx:for-item="item" wx:key="index">
            <view class="cell" style="margin-bottom: 15rpx;">
              <view class="arrange-space-between">
                <t-skeleton theme="text" loading animation="gradient" rowCol="{{ [{ width: '410rpx', height: '38rpx'}]}}"></t-skeleton>
              </view>
              <view class=" arrange-space-between">
                <t-skeleton theme="text" loading animation="gradient" rowCol="{{ [{ width: '135rpx', height: '32rpx'}]}}"></t-skeleton>
                <t-skeleton theme="text" loading animation="gradient" rowCol="{{ [{ width: '300rpx', height: '32rpx'}]}}"></t-skeleton>
              </view>
              <view class="cell-swiper">
                <t-skeleton theme="image" loading animation="gradient" rowCol="{{ [{ width: '695rpx', height: '370rpx'}]}}"></t-skeleton>
              </view>
              <view class="fmr">
                <t-skeleton theme="text" loading animation="gradient" rowCol="{{ [{ height: '112rpx'}]}}"></t-skeleton>
              </view>
              <view class="arrange-column">
                <t-skeleton theme="text" loading animation="gradient" rowCol="{{ [{ width: '120rpx', height: '36rpx'}]}}"></t-skeleton>
                <view class="arrange-space-between" style="margin-top: 12rpx;">
                  <t-skeleton theme="text" loading animation="gradient" rowCol="{{ [{ width: '224rpx', height: '82rpx'}]}}"></t-skeleton>
                  <t-skeleton theme="text" loading animation="gradient" rowCol="{{ [{ width: '224rpx', height: '82rpx'}]}}"></t-skeleton>
                  <t-skeleton theme="text" loading animation="gradient" rowCol="{{ [{ width: '224rpx', height: '82rpx'}]}}"></t-skeleton>
                </view>
              </view>
            </view>
          </view>
        </view>
      </view>
    </view>
  </view>
</template>