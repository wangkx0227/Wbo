.example-search {
  background-color: var(--bg-color-demo);
  padding: 16rpx 32rpx;
  background-color: #ffffff;
}
.page-main{
  margin-top: 8px;
}
.page-main .wrapper .t-cell-group {
  margin-bottom: 20rpx;
}

/* 最后一个元素不需要间距 */
.page-main .wrapper .t-cell-group:last-child {
  margin-bottom: 0;
}

.header-title-main {
  font-size: 12px;
  font-weight: 600;
  color: #333;
}
.header-title-other {
  font-size: 12px;
  color: #606266  ;
}
.header-title {
  display: flex;
  justify-content: space-between; /* 一左一右 */
  align-items: center;
}

/* 重写标签栏组件样式*/
.t-tab-bar--round{
  margin-left: 90rpx !important;
  margin-right: 90rpx !important;
}
.t-tab-bar-item--text-only {
    font-size: 24rpx !important;
}
.t-tab-bar--fixed {
  margin-bottom: 34px !important;
}