.example-search {
  background-color: var(--bg-color-demo);
  padding: 16rpx 32rpx;
  background-color: #ffffff;
}
.page-main{
  margin-top: 8px;
}
.page-main .wrapper .t-cell-group {
  margin-bottom: 20rpx !important;
}

/* 最后一个元素不需要间距 */
.page-main .wrapper .t-cell-group:last-child {
  margin-bottom: 0;
}

.title-main {
  font-size: 12px;
  font-weight: 600;
  color: #333;
}
.title-other {
  font-size: 12px;
  color: #606266  ;
}


/* 重写标签栏组件样式*/
.t-tab-bar {
  z-index: 1999 !important;
}
.t-tab-bar--round{
  margin-left: 90rpx !important;
  margin-right: 90rpx !important;
}
.t-tab-bar-item--text-only {
    font-size: 24rpx !important;
}
.t-tab-bar--fixed {
  margin-bottom: 34px !important;
}

/* top组件调整位置,防止被标签栏组件遮挡 */
.t-back-top {
  margin-bottom: 100px !important;
}
.input-example {
  --td-input-vertical-padding: 24rpx;
  padding: 32rpx 32rpx 16rpx;
}

.input-example-label {
  color: #333;
  font-size: 32rpx;
  line-height: 32rpx;
  margin: 0 8rpx 16rpx;
}

.input-example-input {
  border: 2rpx solid #f5f5f5;
  border-radius: 12rpx;
}
.button-example {
  margin: 32rpx;
  display: flex;
}


.block {
  width: 100vw;
  height: 240px;
  background: var(--td-bg-color-container);
  border-top-left-radius: 16rpx;
  border-top-right-radius: 16rpx;
}

.header {
  display: flex;
  align-items: center;
  height: 116rpx;
}

.title {
  flex: 1;
  text-align: center;
  font-weight: 600;
  font-size: 36rpx;
  color: var(--td-text-color-primary);
}

.btn {
  font-size: 30rpx;
  padding: 30rpx;
}

.btn--cancel {
  color: var(--td-text-color-secondary);
}

.btn--confirm {
  color: #0052d9;
}

.checkbox-scroll-container {
  height: 400rpx; /* 设置固定高度 */
}