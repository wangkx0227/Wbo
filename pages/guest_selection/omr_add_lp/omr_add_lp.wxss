.mb-16 {
  margin-bottom: 32rpx;
}

.input-example {
  --td-input-vertical-padding: 24rpx;
  padding: 32rpx 32rpx 16rpx;
}

.input-example-label {
  color: #333;
  font-size: 32rpx;
  line-height: 32rpx;
  margin: 0 8rpx 16rpx;
}

.input-example-input {
  border: 2rpx solid #f5f5f5;
  border-radius: 12rpx;
}
.button-example {
  margin: 32rpx;
  display: flex;
}
.page-main{
  margin-top: 8px;
}
.page-main .wrapper .t-cell-group {
  margin-bottom: 20rpx !important;
}

/* 最后一个元素不需要间距 */
.page-main .wrapper .t-cell-group:last-child {
  margin-bottom: 0;
}

/* top组件调整位置,防止被标签栏组件遮挡 */
.t-back-top {
  margin-bottom: 100px !important;
}

