

.page-main{
  margin-top: 5px;
  padding: 5px;
}
.page-main .wrapper .cell {
  margin-bottom: 15rpx;
  background-color: #ffffff;
  padding: 5px 10px;
}

/* 最后一个元素不需要间距 */
.page-main .wrapper .cell:last-child {
  margin-bottom: 0;
}
.cell-swiper{
  margin-top: 5px;
  margin-bottom: 10px;
  position: relative; 
}

.header-title-main {
  font-size: 14px;
  font-weight: 600;
  color: #333;
}
.header-title-other {
  font-size: 12px;
  color: #606266  ;
}
.header-title {
  display: flex;
  justify-content: space-between; /* 一左一右 */
  align-items: center;
}
.header-button{
  display: flex;
  align-items: center;
}
.header-button-other{
  font-size: 14px;
  color: #606266  ;
}



.grid-overlay {
  position: absolute; /* 绝对定位叠加 */
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  grid-template-rows: repeat(3, 1fr);
  pointer-events: none;
}

.grid-cell {
  border: 0.5px solid rgba(255, 255, 255, 0.5);
  box-sizing: border-box;
}
